<script type="text/javascript">  $(function() {    new Highcharts.Chart({      chart:   { type: "scatter", renderTo: "results-chart" },      title:   { text: "Results by Age" },      credits: { enabled: false },      tooltip: { formatter: function() { return this.point.name; } },      yAxis:   { title: { text: "Minutes" }, lineWidth: 1 },      xAxis:   { title: { text: "Age" } },      legend:  {         align: 'right',        verticalAlign: 'top',        x: -10,        y: 50,        floating: true },      series: [        {          name: "male",          data: [<%= series_data(@race.results.male) %>]        },{          name: 'female',          data: [<%= series_data(@race.results.female) %>]        }]    }); // end chart()            $('#btn-show-list').click(function() {          $('#results-list').show();          $('#results-chart').hide();      });      $('#btn-show-chart').click(function() {          $('#results-list').hide();          $('#results-chart').show();      });    }); // end ready()</script><div class="panel">    <h3><%=h @race.name %></h3>    <div class="panel-content">        <p>            <%=h @race.description %>        </p>        <p>            <b>finishers:</b>            <%=h @race.finisher_count %>        </p>        <p>            <b>average male time:</b>            <%=h seconds_to_time(@race.results.male.average('gun_time')) %>        </p>        <p>            <b>average female time:</b>            <%=h seconds_to_time(@race.results.female.average('gun_time')) %>        </p>    </div></div><br><% if @race.results.empty? %>    <div class="panel">        <h3>load results</h3>        <div class="panel-content">            <%= form_tag(load_results_path(id: @race), multipart: true, class: 'form-horizontal') do %>                <div class="control-group">                    <label class="control-label" for="race_datafile">Select File</label>                    <div class="controls">                        <%= file_field_tag('datafile') %>                    </div>                </div>                <div class="control-group">                    <label class="control-label" for="first_name_index">first name</label>                    <div class="controls">                        <%= text_field_tag('first_name_index') %>                    </div>                </div>                <div class="control-group">                    <label class="control-label" for="last_name_index">last name</label>                    <div class="controls">                        <%= text_field_tag('last_name_index') %>                    </div>                </div>                <div class="control-group">                    <label class="control-label" for="city_index">city</label>                    <div class="controls">                        <%= text_field_tag('city_index') %>                    </div>                </div>                <div class="control-group">                    <label class="control-label" for="club_index">club</label>                    <div class="controls">                        <%= text_field_tag('club_index') %>                    </div>                </div>                <div class="control-group">                    <label class="control-label" for="age_index">age</label>                    <div class="controls">                        <%= text_field_tag('age_index') %>                    </div>                </div>                <div class="control-group">                    <label class="control-label" for="bib_index">bib</label>                    <div class="controls">                        <%= text_field_tag('bib_index') %>                    </div>                </div>                <div class="control-group">                    <label class="control-label" for="div_index">div</label>                    <div class="controls">                        <%= text_field_tag('div_index') %>                    </div>                </div>                <div class="control-group">                    <label class="control-label" for="div_place_index">div place</label>                    <div class="controls">                        <%= text_field_tag('div_place_index') %>                    </div>                </div>                <div class="control-group">                    <label class="control-label" for="overall_place_index">overall place</label>                    <div class="controls">                        <%= text_field_tag('overall_place_index') %>                    </div>                </div>                <div class="control-group">                    <label class="control-label" for="gun_time_index">gun time</label>                    <div class="controls">                        <%= text_field_tag('gun_time_index') %>                    </div>                </div>                <div class="control-group">                    <label class="control-label" for="chip_time_index">chip time</label>                    <div class="controls">                        <%= text_field_tag('chip_time_index') %>                    </div>                </div>                <div class="control-group">                    <label class="control-label" for="penalty_time_index">penalty time</label>                    <div class="controls">                        <%= text_field_tag('penalty_time_index') %>                    </div>                </div>                <%= submit_tag 'upload', :class => 'btn btn-primary pull-right' %>                <div class="clear"></div>            <% end %>        </div>    </div> <!-- end .panel --><% else %>    <div class="btn-group" data-toggle="buttons-radio">      <button type="button" id="btn-show-chart" class="btn btn-primary active">chart</button>      <button type="button" id="btn-show-list" class="btn btn-primary">list</button>    </div><p>    <div id="results-chart" style="width:700px; height:500px;"></div>    <div id="results-list" class="well hide">          <%= render('results') %>    </div><% end %>