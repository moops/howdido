<script type="text/javascript">  $(function() {    new Highcharts.Chart({      chart:   { type: "scatter", renderTo: "results-chart" },      title:   { text: "Results by Age" },      credits: { enabled: false },      tooltip: { formatter: function() { return this.point.name; } },      yAxis:   { title: { text: "Minutes" }, lineWidth: 1 },      xAxis:   { title: { text: "Age" } },      legend:  {         align: 'right',        verticalAlign: 'top',        x: -10,        y: 50,        floating: true },      series: [        {          name: "male",          data: [<%= series_data(@race.results.male) %>]        },{          name: 'female',          data: [<%= series_data(@race.results.female) %>]        }]    }); // end chart()            $('#btn-show-list').click(function() {          $('#results-list').show();          $('#results-chart').hide();      });      $('#btn-show-chart').click(function() {          $('#results-list').hide();          $('#results-chart').show();      });    }); // end ready()</script><div class="panel">    <h3><%=h @race.name %></h3>    <div class="panel-content">        <p>            <%=h @race.description %>        </p>        <p>            <b>finishers:</b>            <%=h @race.finisher_count %>        </p>        <p>            <b>average male time:</b>            <%=h seconds_to_time(@race.results.male.average('gun_time')) %>        </p>        <p>            <b>average female time:</b>            <%=h seconds_to_time(@race.results.female.average('gun_time')) %>        </p>    </div></div><br><% if @race.results.empty? %>    <div class="panel">        <h3>load results</h3>        <div class="panel-content">            <%= render(partial: 'results/load_form', locals: { race: @race }) %>        </div>    </div> <!-- end .panel --><% else %>    <div class="btn-group" data-toggle="buttons-radio">      <button type="button" id="btn-show-chart" class="btn btn-primary active">chart</button>      <button type="button" id="btn-show-list" class="btn btn-primary">list</button>    </div><p>    <div id="results-chart" style="width:700px; height:500px;"></div>    <div id="results-list" class="well hide">          <%= render 'results' %>    </div><% end %>